{
    "name": "awes-io/awes-io",
    "type": "project",
    "description": "Awes.IO build based on Laravel Framework.",
    "keywords": [
        "awes-io",
        "framework",
        "laravel",
        "vuejs",
        "vuejs2"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "awes-io/auth": "^1.1",
        "awes-io/base-js": "^1.6",
        "awes-io/context-menu": "^1.0",
        "awes-io/filter-wrapper": "^1.0",
        "awes-io/form-builder": "^1.3",
        "awes-io/generator": "^0.0",
        "awes-io/indigo-layout": "^1.10",
        "awes-io/localization-helper": "^1.0",
        "awes-io/modal-window": "^1.1",
        "awes-io/repository": "^1.0",
        "awes-io/table-builder": "^1.0",
        "begimov/thanks": "^1.1",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.pkgkit.com",
            "options": {
                "http": {
                    "header": [
                        "API-TOKEN: 05bc8d7f3eca8066d109001d79ebaa5dda4dac971db5e906e1a6ace742e83fe3"
                    ]
                }
            }
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "awes-io-thanks": {
            "awes-io/awes-io": "https://github.com/awes-io/awes-io.git",
            "awes-io/form-builder": "https://github.com/awes-io/form-builder.git",
            "awes-io/indigo-layout": "https://github.com/awes-io/indigo-layout.git",
            "awes-io/table-builder": "https://github.com/awes-io/table-builder.git",
            "awes-io/navigator": "https://github.com/awes-io/navigator",
            "awes-io/repository": "https://github.com/awes-io/repository.git",
            "awes-io/auth": "https://github.com/awes-io/auth.git",
            "awes-io/localization-helper": "https://github.com/awes-io/localization-helper"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
